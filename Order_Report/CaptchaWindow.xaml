<Window x:Class="Order_Report.CaptchaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Order_Report"
        mc:Ignorable="d"
        Title="CaptchaWindow" Height="300" Width="400" Background="#d7bbaa">
    <Grid>
        <Image x:Name="CaptchaImage" Width="340" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="23,35,0,0"/>
        <TextBox x:Name="CaptchaInput" Width="189" Height="23" HorizontalAlignment="Center" Margin="0,170,0,0"/>
        <Button Content="OK" Width="100" Height="23" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,210,0,0" Click="Button_CaptchaOK_Click"/>
    </Grid>
</Window>
